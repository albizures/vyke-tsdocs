{
	"name": "@vyke/tsdocs",
	"type": "module",
	"version": "0.0.2",
	"packageManager": "pnpm@8.11.0",
	"author": "Jose Albizures <jose@albizures.com> (https://albizures.com/)",
	"license": "MIT",
	"keywords": [],
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		},
		"./cli": {
			"import": "./dist/cli/index.mjs",
			"require": "./dist/cli/index.cjs"
		},
		"./package.json": "./package.json"
	},
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.ts",
	"bin": {
		"tsdocs": "./dist/cli/index.mjs"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "vite-node ./src/cli/index.ts",
		"build": "unbuild",
		"test": "run-s test:unit test:types test:lint",
		"test:use": "knip",
		"test:lint": "eslint .",
		"test:unit": "vitest run",
		"test:unit:dev": "vitest",
		"test:types": "tsc --noEmit",
		"prepack": "pnpm build",
		"release:bump": "standard-version -t v",
		"release:publish": "pnpm publish --access public",
		"release:push-tags": "git push --follow-tags origin main",
		"release": "run-s test build release:bump release:publish release:push-tags"
	},
	"peerDependencies": {
		"typescript": "^5.*"
	},
	"dependencies": {
		"@vyke/results": "^0.0.5",
		"@vyke/sola": "^0.0.5",
		"citty": "^0.1.5"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^2.6.1",
		"@types/node": "^20.10.6",
		"eslint": "^8.56.0",
		"knip": "^3.9.0",
		"npm-run-all": "^4.1.5",
		"standard-version": "^9.5.0",
		"typescript": "^5.3.3",
		"unbuild": "^2.0.0",
		"vite-node": "^1.1.1",
		"vitest": "^1.1.1"
	},
	"standard-version": {
		"skip": {
			"changelog": true
		}
	}
}
